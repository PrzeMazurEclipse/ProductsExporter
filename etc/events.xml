<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="export_success">
        <observer name="export_success" instance="YellowCard\ProductsExporter\Observer\ExportSuccess"/>
    </event>
    <event name="export_failed">
        <observer name="export_failed" instance="YellowCard\ProductsExporter\Observer\ExportFailed"/>
    </event>
</config>
